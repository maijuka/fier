<?xml version="1.0" encoding="utf-8"?>
<root>
  <field_spec>
    <name>acc_ratioTL</name>
    <expression>test:acc:/learn:acc:</expression>
    <format>.2f</format>
  </field_spec>
  <field_spec>
    <name>len_I_ratioTA</name>
    <expression>test:len:I/(1.*all:len:I)</expression>
    <format>.2f</format>
  </field_spec>
  <field_spec>
    <name>cohesion</name>
    <expression>:extra:cohesion</expression>
    <format>.2f</format>
  </field_spec>
  <placeholder_spec>
    <name>Exm</name>
    <values>Ex*</values>
    <if>wmissing</if>
    <then><values>Em*</values></then>
  </placeholder_spec>
  <placeholder_spec>
    <name>folds</name>
    <if>wfolds</if>
    <then><values>test,learn</values></then>
    <else><values>all</values></else>
    <if>wcond</if>
    <then><values>cond</values></then>
  </placeholder_spec>
  <placeholder_spec>
    <name>folds_active</name>
    <if>wfolds</if>
    <then><values>test,learn</values></then>
    <else><values>active</values></else>
    <if>wcond</if>
    <then><values>cond</values></then>
  </placeholder_spec>

  <fieldlist>
    <name>cols</name>
    <field><name>LHS:Cset:</name></field>
    <field><name>RHS:Cset:</name></field>
    <field><name>COND:Cset:</name><if>wcond</if></field>
  </fieldlist>
  <fieldlist>
    <name>queries</name>
    <field><name>LHS:query:</name></field>
    <field><name>RHS:query:</name></field>
    <field><name>COND:query:</name><if>wcond</if></field>
  </fieldlist>
  <fieldlist>
    <name>cardsall</name>
    <field><name>all:card:Y</name>
    <placeholder><token>Y</token><name>Exm</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>cards</name>
    <field>
      <name>X:card:Y</name>
      <placeholder><token>Y</token><name>Exm</name></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field>
      <name>X:card:Y</name>
      <placeholder><token>Y</token><values>L,R,A,B,U,I</values></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>percsall</name>
    <field>
      <name>all:perc:Y</name>
      <placeholder><token>Y</token><name>Exm</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>percs</name>
    <field>
      <name>X:perc:Y</name>
      <placeholder><token>Y</token><name>Exm</name></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field>
      <name>X:perc:Y</name>
      <placeholder><token>Y</token><values>L,R,A,B,U,I</values></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>suppsall</name>
    <field>
      <name>all:supp:Y</name>
      <placeholder><token>Y</token><name>Exm</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>supps</name>
    <field>
      <name>X:supp:Y</name>
      <placeholder><token>Y</token><name>Exm</name></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field>
      <name>X:supp:Y</name>
      <placeholder><token>Y</token><values>L,R,A,B,U,I</values></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
  </fieldlist>
  <fieldlist>
    <name>areas</name>
    <field>
      <name>X:area:Y</name>
      <placeholder><token>Y</token><values>L,R,A,B,U,I</values></placeholder>
      <placeholder><token>X</token>
      <if>wfolds</if>
      <then><values>test,learn</values></then>
      <else><values>all,active</values></else>
      </placeholder>
    </field>
  </fieldlist>

  <fieldlist>
    <name>statsall</name>
    <field><name>all:acc:</name></field>
    <field><name>all:len:I</name></field>
    <field><name>all:pval:</name></field>
  </fieldlist>

  <fieldlist>
    <name>stats</name>
    <field><name>X</name><if>wfolds</if>
    <placeholder><token>X</token><values>acc_ratioTL,len_I_ratioTA</values></placeholder>
    </field>
    <field>
      <name>X:acc:</name>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field>
      <name>X:len:I</name>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field>
      <name>X:pval:</name>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
  </fieldlist>

  <fieldlist>
    <name>statsactive</name>
    <field><name>X</name><if>wfolds</if>
    <placeholder><token>X</token><values>acc_ratioTL,len_I_ratioTA</values></placeholder>
    </field>
    <field>
      <name>X:acc:</name>
      <placeholder><token>X</token><name>folds_active</name></placeholder>
    </field>
    <field>
      <name>X:len:I</name>
      <placeholder><token>X</token><name>folds_active</name></placeholder>
    </field>
    <field>
      <name>X:perc:I</name>
      <placeholder><token>X</token><name>folds_active</name></placeholder>
    </field>
    <field>
      <name>X:pval:</name>
      <placeholder><token>X</token><name>folds_active</name></placeholder>
    </field>
  </fieldlist>

  <fieldlist>
    <!-- fields list for text export -->
    <name>basic</name>
    <field><name>:rid:</name></field>
    <list><name>queries</name></list>
    <field>
      <name>X:Y:</name>
      <placeholder><token>Y</token><values>acc,pval</values></placeholder>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <field><name>X</name><if>wfolds</if>
    <placeholder><token>X</token><values>acc_ratioTL,len_I_ratioTA</values></placeholder>
    </field>
    <field><name>cohesion</name><if>wxtr_geoplus</if></field>
    <list><name>cardsall</name></list>
  </fieldlist>

  <fieldlist>
    <!-- fields list for TeX export -->
    <name>tex</name>
    <list><name>queries</name></list>
    <field>
      <name>X:acc:</name>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <!--<field>
        <name>X:len:I</name>
        <placeholder><token>X</token><name>folds</name></placeholder>
        </field>-->
    <field>
      <name>X:perc:I</name>
      <placeholder><token>X</token><name>folds</name></placeholder>
    </field>
    <!--<field>
        <name>X:pval:</name>
        <placeholder><token>X</token><name>folds</name></placeholder>
        </field>-->
  </fieldlist>

  <fieldlist>
    <!-- fields list for the graphical user interface Siren -->
    <name>gui</name>
    <list><name>queries</name></list>
    <field><name>X</name><if>wfolds</if>
    <placeholder><token>X</token><values>acc_ratioTL,len_I_ratioTA</values></placeholder>
    </field>
    <field><name>cohesion</name><if>wxtr_geoplus</if></field>
    <list><name>statsactive</name></list>
  </fieldlist>

</root>
